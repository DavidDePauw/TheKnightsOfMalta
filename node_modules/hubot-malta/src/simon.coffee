#test_file

module.exports = (robot) ->
  robot.enter (msg) ->  
    msg.reply "Hello.  Is it me you're looking for?"
  robot.respond /yoyo/i, (msg) ->
    msg.send "Yoyoyo"
  robot.respond /My skill is:\s+(.*)/i, (res) ->
    robot.brain.set 'skill:#{msg.envelope.user.name}', res.match[1]
    res.send "Roger."
  robot.respond What is my skill?, (res) ->
    skill=robot.brain.get 'skill:#{msg.envelope.user.name}'
    res.send skill
 
